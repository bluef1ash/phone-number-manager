<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.phonenumbermanager.mapper.RolePrivilegeRelationMapper">
    <resultMap type="com.github.phonenumbermanager.entity.RolePrivilegeRelation" id="baseResultMap">
        <id column="role_id" property="roleId"/>
        <id column="privilege_id" property="privilegeId"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>
    <delete id="deleteByRolePrivilegeRelation">
        DELETE FROM `pm_role_privilege`
        <where>
            <if test="roleId != null">
                `role_id` = #{roleId}
            </if>
            <if test="privilegeId != null">
                AND `privilege_id` = #{privilegeId}
            </if>
            <if test="createTime != null">
                AND `create_time` = #{createTime}
            </if>
            <if test="updateTime != null">
                AND `update_time` = #{updateTime}
            </if>
        </where>
    </delete>
</mapper>
