{
  "name": "ant-design-pro",
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --custom-syntax postcss",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none NODE_OPTIONS=--openssl-legacy-provider UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit",
    "update": "ncu -u && npm install",
    "preinstall": "npx only-allow npm"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.20.21",
    "@ant-design/pro-descriptions": "^1.12.6",
    "@ant-design/pro-form": "^1.74.6",
    "@ant-design/pro-layout": "^6.38.22",
    "@ant-design/pro-table": "^2.80.6",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.4",
    "@types/js-cookie": "^3.0.2",
    "@umijs/route-utils": "^2.2.0",
    "antd": "^4.22.8",
    "classnames": "^2.3.1",
    "js-cookie": "^3.0.1",
    "libphonenumber-js": "^1.10.13",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "omit.js": "^2.0.2",
    "only-allow": "^1.1.1",
    "qs": "^6.11.0",
    "rc-menu": "^9.6.3",
    "rc-util": "^5.24.2",
    "react": "^18.2.0",
    "react-dev-inspector": "^1.8.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "umi": "^3.5.32",
    "umi-serve": "^1.9.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/history": "^5.0.0",
    "@types/jest": "^29.0.1",
    "@types/lodash": "^4.14.185",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@umijs/fabric": "^3.0.0",
    "@umijs/openapi": "^1.7.0",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/plugin-openapi": "^1.3.3",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.47",
    "@umijs/preset-react": "^2.1.4",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.23.1",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "express": "^4.18.1",
    "gh-pages": "^4.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "puppeteer-core": "^17.1.3",
    "stylelint": "^14.11.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "swagger-ui-react": "^4.13.2",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
