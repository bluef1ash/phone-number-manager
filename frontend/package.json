{
  "name": "ant-design-pro",
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --custom-syntax postcss",
    "openapi": "umi openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit",
    "update": "ncu -u && npm update"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.18.34",
    "@ant-design/pro-descriptions": "^1.10.47",
    "@ant-design/pro-form": "^1.55.1",
    "@ant-design/pro-layout": "^6.33.1",
    "@ant-design/pro-table": "^2.65.0",
    "@umijs/route-utils": "^2.0.5",
    "antd": "^4.18.9",
    "classnames": "^2.3.1",
    "libphonenumber-js": "^1.9.49",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.3.2",
    "rc-util": "^5.18.1",
    "react": "^17.0.2",
    "react-dev-inspector": "^1.7.1",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "umi": "^3.5.21",
    "umi-serve": "^1.9.10"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.11",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@umijs/fabric": "^2.10.2",
    "@umijs/openapi": "^1.4.1",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/plugin-openapi": "^1.3.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.39",
    "@umijs/preset-react": "^2.1.1",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.10.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "express": "^4.17.3",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.3.4",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "puppeteer-core": "^13.4.1",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "swagger-ui-react": "^4.6.1",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
